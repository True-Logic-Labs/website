{
  "name": "🔔 Release Notes & Updates Assistant",
  "description": "Keeping your users and stakeholders informed about the latest updates, improvements, and bug fixes is essential for product transparency and user satisfaction. This category provides powerful tools to craft compelling release notes, changelogs, feature highlights, and upgrade instructions, helping you communicate changes clearly and effectively across various channels. Streamline your communication strategy and ensure everyone stays in the loop with professional, well-structured content.",
  "tools": [
    {
      "name": "Version Release Note Generator",
      "description": "Generate professional and concise release notes highlighting new features, bug fixes, and improvements for each product version.",
      "input_needed": ["Version number", "List of new features", "Bug fixes", "Improvements"]
    },
    {
      "name": "Feature Highlight Writer",
      "description": "Create engaging summaries of key new features that emphasize user benefits and usage instructions.",
      "input_needed": ["Feature description", "User benefits", "Usage instructions"]
    },
    {
      "name": "Bug Fix Summary Generator",
      "description": "Craft detailed but user-friendly explanations of resolved issues and performance improvements.",
      "input_needed": ["Bug details", "Impact of bugs", "Resolution details"]
    },
    {
      "name": "Changelog Entry Writer",
      "description": "Automatically generate structured changelog entries that are clear and easy to scan for users and stakeholders.",
      "input_needed": ["Version number", "Change types (new, fix, improvement)", "Descriptions of changes"]
    },
    {
      "name": "Upgrade Instructions Content Generator",
      "description": "Provide step-by-step upgrade guides for users, detailing migration requirements and version compatibility.",
      "input_needed": ["Current version", "Target version", "Steps for upgrade", "Migration requirements"]
    },
    {
      "name": "Internal Stakeholder Announcement Template",
      "description": "Generate tailored content to inform internal teams, such as sales or support, about the latest product updates.",
      "input_needed": ["Audience type", "Summary of updates", "Key impact points"]
    },
    {
      "name": "Social Media Update Snippet Generator",
      "description": "Create concise, engaging text snippets for social media announcements related to product updates.",
      "input_needed": ["Platform (Twitter, LinkedIn, etc.)", "Key message", "Tone of voice"]
    }
  ]
}




























